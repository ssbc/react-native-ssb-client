{
  "name": "react-native-ssb-client",
  "version": "1.0.0",
  "description": "Secure Scuttlebutt client-side API for React Native apps",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:staltz/react-native-ssb-client.git"
  },
  "scripts": {
    "compile": "tsc",
    "prepublishOnly": "npm run compile",
    "install": "node insert-shims.js && npm run link",
    "link": "cd ../../ && react-native link react-native-randombytes && react-native link react-native-crypto && react-native link react-native-fs && react-native link react-native-os && react-native link react-native-tcp",
    "prepare": "patch-package"
  },
  "author": "Andre Staltz",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^8.0.9",
    "patch-package": "^3.2.1",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "path": "0.12.x",
    "process-shim": "file:./process-shim",
    "react-native-crypto": "^2.0.2",
    "react-native-fs": "2.3.x",
    "react-native-os": "^1.1.0",
    "react-native-tcp": "3.2.x",
    "react-native-randombytes": "2.2.0",
    "sodium-browserify-tweetnacl": "^0.2.3",
    "ssb-client": "4.5.x",
    "ssb-config": "2.2.x",
    "ssb-keys": "7.0.10",
    "stream-browserify": "^2.0.1",
    "url": "0.11.0",
    "vm-browserify": "^0.0.4"
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "chloride": "sodium-browserify-tweetnacl",
    "fs": false,
    "net": "react-native-tcp",
    "os": "react-native-os",
    "stream": "stream-browserify",
    "process": "process-shim",
    "url": "url",
    "vm": "vm-browserify"
  }
}
